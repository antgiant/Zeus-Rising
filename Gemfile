source "https://rubygems.org"

# Use the GitHub Pages gem so local `bundle exec jekyll build` mirrors the Pages environment.
# This pins Jekyll and common plugins to the versions GitHub Pages uses.
gem "github-pages", group: :jekyll_plugins

# If you need extra plugins not provided by `github-pages`, add them below
# and keep them in the `:jekyll_plugins` group. Example:
# group :jekyll_plugins do
#   gem "some-other-plugin"
# end
